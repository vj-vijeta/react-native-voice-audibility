{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:android": {
      "inputs": [
        "package.json",
        "android",
        "!android/build",
        "src/**",
        "src/*.ts",
        "src/*.tsx"
      ],
      "outputs": ["android/build","dist"]
    },
    "build:ios": {
      "inputs": [
        "package.json",
        "ios",
        "!ios/build",
        "src/*.ts",
        "src/*.tsx"
      ],
      "outputs": ["ios/build"]
    }
  }
}